{
  "name": "readToShareSystem",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "debug": "sudo node --debug ./bin/www",
    "start": "sudo supervisor ./bin/www",
    "webpack": "sudo webpack --progress --color --watch",
    "inspector": "node-inspector",
    "livereload": "livereload",
    "reset": "mysql -uroot -pddainn1314 < db/create.sql",
    "backup": "mysqldump -uroot -pddainn1314 readToShare > db/backup.sql",
    "backin": "mysql -uroot -pddainn1314 readToShare < db/backup.sql"
  },
  "dependencies": {
