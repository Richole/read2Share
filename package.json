    "connect-multiparty": "~2.0.0",
    "cookie-parser": "~1.3.5",
    "crypto": "0.0.3",
    "css-loader": "~0.23.1",
    "debug": "~2.2.0",
    "ejs": "~2.3.4",
    "exports-loader": "~0.6.3",
    "expose-loader": "~0.7.1",
    "express": "~4.13.1",
    "express-session": "~1.13.0",
    "file-loader": "~0.8.5",
    "imports-loader": "~0.6.5",
    "jade": "~1.11.0",
    "jsx-loader": "~0.13.2",
    "less-loader": "~2.2.2",
    "log4js": "^0.6.33",
    "mongoose": "~4.3.5",
    "morgan": "~1.6.1",
    "mysql": "^2.10.2",
    "node-mysql": "~0.4.2",
    "nodemailer": "~0.7.1",
    "request": "~2.69.0",
    "sass-loader": "~3.1.2",
